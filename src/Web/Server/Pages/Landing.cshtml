            <div class="section-grid featured-grid">
                @foreach (var podcast in Model.FeaturedShows ?? new Show[] { })
                {
                    <div class="featured-item">
                        <picture class="featured-item__picture">
                            <img src="@podcast.Image" alt="@podcast.Title" loading="lazy" />
                        </picture>
                        <h3 class="featured-item__title">@podcast.Title</h3>
                        <p class="featured-item__author">@podcast.Author</p>
                    </div>
